<div id="message_new">
  <%= form_for @message do |f| %>
      <!--TODO place for errors-->
      <%= label_tag :to %>
      <%= text_field_tag :to, params[:to] %>
      <br/>
      <%= f.label :subject %>
      <%= f.text_field :subject %>
      <br/>
      <%= f.label :body %>
      <%= f.text_area :body %>
      <br/>
      <%= f.submit 'Save' %>
      <%= link_to 'Back', messages_path %>
  <% end %>
</div>